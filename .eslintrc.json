{
  "extends": "eslint:recommended",
  "rules": {
    "semi" : [2, "always"],
    "no-console": 0,
    "no-unused-vars": 0,
    "comma-dangle": [2, "never"] 
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "angular": true
  }
}
